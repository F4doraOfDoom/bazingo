	"Prototype_Comment": {
		"prefix": "protocomment",
		"scope": "c",
		"body": [
			"/*",
			" * ${0:TODO: Description}",
			" * Input:",
			" * \t${1:none}",
			" * Output:",
			" * \t${2:none}",
			" */"
		],
		"description": "Comment before function prototype."
	},
	"Function_Comment": {
		"prefix": "funccomment",
		"scope": "c",
		"body": [
			"/*",
			" * Assuming:",
			" * \tnone",
			" * Algorithm:",
